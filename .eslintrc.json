{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "accessor-pairs": 1,
        "array-bracket-spacing": 1,
        "array-callback-return": 1,
        "arrow-body-style": 1,
        "arrow-parens": 1,
        "arrow-spacing": 1,
        "block-scoped-var": 1,
        "block-spacing": 1,
        "brace-style": 1,
        "callback-return": 1,
        "camelcase": 0,
        "class-methods-use-this": 1,
        "comma-dangle": 1,
        "comma-spacing": 0,
        "comma-style": 1,
        "complexity": 1,
        "computed-property-spacing": 1,
        "consistent-return": 1,
        "consistent-this": 1,
        "curly": 1,
        "default-case": 1,
        "dot-location": 1,
        "dot-notation": [
            1,
            {
                "allowKeywords": true
            }
        ],
        "eol-last": 1,
        "eqeqeq": 1,
        "func-call-spacing": 1,
        "func-names": [
            1,
            "never"
        ],
        "func-style": [
            1,
            "expression"
        ],
        "generator-star-spacing": 1,
        "global-require": 1,
        "guard-for-in": 1,
        "handle-callback-err": 1,
        "id-blacklist": 1,
        "id-length": 0,
        "id-match": 1,
        "indent": 0,
        "init-declarations": 1,
        "jsx-quotes": 1,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "line-comment-position": 1,
        "linebreak-style": [
            1,
            "unix"
        ],
        "lines-around-comment": 1,
        "lines-around-directive": 1,
        "max-depth": 1,
        "max-len": 0,
        "max-lines": 1,
        "max-nested-callbacks": 1,
        "max-params": 1,
        "max-statements": 1,
        "max-statements-per-line": 1,
        "multiline-ternary": 1,
        "new-cap": 1,
        "new-parens": 1,
        "newline-after-var": [
            1,
            "always"
        ],
        "newline-before-return": 0,
        "newline-per-chained-call": 1,
        "no-alert": 1,
        "no-array-constructor": 1,
        "no-bitwise": 1,
        "no-caller": 1,
        "no-catch-shadow": 1,
        "no-confusing-arrow": 1,
				"no-console": 0,
        "no-continue": 1,
        "no-div-regex": 1,
        "no-duplicate-imports": 1,
        "no-else-return": 1,
        "no-empty-function": 0,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-label": 1,
        "no-extra-parens": 1,
        "no-floating-decimal": 1,
        "no-global-assign": 1,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 1,
        "no-implied-eval": 1,
        "no-inline-comments": 1,
        "no-inner-declarations": [
            1,
            "functions"
        ],
        "no-invalid-this": 1,
        "no-iterator": 1,
        "no-label-var": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-lonely-if": 1,
        "no-loop-func": 1,
        "no-magic-numbers": 0,
        "no-mixed-operators": 1,
        "no-mixed-requires": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-multiple-empty-lines": 0,
        "no-negated-condition": 1,
        "no-nested-ternary": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-wrappers": 1,
        "no-octal-escape": 1,
        "no-param-reassign": 1,
        "no-path-concat": 0,
        "no-plusplus": [
            1,
            {
                "allowForLoopAfterthoughts": true
            }
        ],
        "no-process-env": 1,
        "no-process-exit": 1,
        "no-proto": 1,
        "no-prototype-builtins": 1,
        "no-restricted-globals": 1,
        "no-restricted-imports": 1,
        "no-restricted-modules": 1,
        "no-restricted-properties": 1,
        "no-restricted-syntax": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-shadow": 1,
        "no-shadow-restricted-names": 1,
        "no-spaced-func": 1,
        "no-sync": 1,
        "no-tabs": 1,
        "no-template-curly-in-string": 1,
        "no-ternary": 1,
        "no-throw-literal": 1,
        "no-trailing-spaces": 1,
        "no-undef-init": 1,
        "no-undefined": 1,
        "no-underscore-dangle": 1,
        "no-unmodified-loop-condition": 1,
        "no-unneeded-ternary": 1,
        "no-unsafe-negation": 1,
        "no-unused-expressions": 1,
        "no-use-before-define": 1,
        "no-useless-call": 1,
        "no-useless-computed-key": 1,
        "no-useless-concat": 1,
        "no-useless-constructor": 1,
        "no-useless-escape": 1,
        "no-useless-rename": 1,
        "no-var": 0,
        "no-void": 1,
        "no-warning-comments": 1,
        "no-whitespace-before-property": 1,
        "no-with": 1,
        "object-curly-newline": 1,
        "object-curly-spacing": 1,
        "object-property-newline": 1,
        "object-shorthand": 1,
        "one-var": 0,
        "one-var-declaration-per-line": 1,
        "operator-assignment": [
            1,
            "always"
        ],
        "operator-linebreak": 1,
        "padded-blocks": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 1,
        "prefer-numeric-literals": 1,
        "prefer-reflect": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "prefer-template": 0,
        "quote-props": 1,
        "quotes": [
            1,
            "single"
        ],
        "radix": 1,
        "require-jsdoc": 1,
        "rest-spread-spacing": 1,
        "semi": 0,
        "semi-spacing": [
            1,
            {
                "after": true,
                "before": false
            }
        ],
        "sort-imports": 1,
        "sort-keys": 1,
        "sort-vars": 1,
        "space-before-blocks": 0,
        "space-before-function-paren": 0,
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": [
            1,
            "always"
        ],
        "strict": [
            1,
            "never"
        ],
        "symbol-description": 1,
        "template-curly-spacing": 1,
        "unicode-bom": [
            1,
            "never"
        ],
        "valid-jsdoc": 1,
        "vars-on-top": 0,
        "wrap-iife": 1,
        "wrap-regex": 1,
        "yield-star-spacing": 1,
        "yoda": 1
    }
}
