{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals" : {
      "angular" : 1,
      "app" : 1
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
				"no-console": [
					"off"
				]

    }
}
